@model List<StackOverflowProject.ViewModels.QuestionViewModel>
@{ 
    ViewBag.Title = "Home";
}

<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            <h1 style="text-align:center">Learn, Shared, Build!</h1><br />
            <h3 style="text-align:center;font-family: monospace">Each month, over 50 million developers come to Stack Overflow to 
            learn, share their knowledge and build their careers</h3><br />
            <h5 style="text-align:center">Join the world's largest developer community.</h5>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-12">
       <table class="table">
           @foreach(var item in Model)
           {
               Html.RenderPartial("Question", item);
           }
       </table>
        @if(Session["CurrentUserName"]!=null)
        {
            <a href="/questions/create" class="btn btn-primary">Add Question</a>
        }
    </div>
</div>

<script>
    $("a[href='/home/index']").attr("id", "active");
</script>